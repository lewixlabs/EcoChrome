{
    "name": "Eco Chrome",
    "description": "Cool & Green Screen Saver for Chrome!",
    "version": "1.0.9",
    "app": {
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
    },
    "permissions": [
        "alarms",
        "storage",
        "idle",
        "overrideEscFullscreen",
        "app.window.alwaysOnTop"
